x:int .plus y:int | int  = (ffi.javascript "_x + _y") : int;

intlist = (_.tag nil).or(_.tag cons carry (record.current int next intlist));

myList=cons.carry(record.current 1 next (cons.carry(record.current 2 next (cons.carry(record.current 3 next nil)))));


this:intlist.length | int = 
    this.
        nil? 0 
        cons? (1 .plus (this.carry.next.length));

this:intlist.sum | int = 
    this.
        nil? 0
        cons? (this.carry.current.plus(this.carry.next.sum));

=myList.length;
=myList.sum;
