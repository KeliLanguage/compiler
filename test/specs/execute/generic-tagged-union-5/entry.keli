List.of(A Type) = tags.
    case(.Nil)
    case(.Cons($.value(A) next(List.of(A))))

Box.of(A Type) = tags.
    case(.With(A))
    case(.Empty)

"Get the last element of a list"
{A Type}
(this List.of(A)).last
    | Box.of(A)
    = this.
        case(.Nil):
            (Box.Empty)
        case(.Cons(c)):
            (c.next.
                case(.Nil):
                    (Box.With(c.value))
                default:
                    (Box.Empty))

= List.Cons($.value(1) next(List.Nil)).last