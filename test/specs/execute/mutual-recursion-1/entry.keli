Boolean = choice
    .True
    .False

(x Int).==(y Int) = ffi.javascript("k$x === k$y ? k$Boolean.True : k$Boolean.False").as(Boolean)
(x Int).-(y Int) = ffi.javascript("k$x - k$y").as(Int)
(this Int).isEven | Boolean = 
    this.==(0).
        case(.True):
            (Boolean.True)
        case(.False):
            (this.-(1).isOdd)

(this Int).isOdd | Boolean = 
    this.==(0).
        case(.True):
            (Boolean.False)
        case(.False):
            (this.-(1).isEven)

= 20.isEven
= 9.isOdd
= 8.isOdd
