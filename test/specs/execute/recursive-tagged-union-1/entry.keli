(x Int).plus(y Int) | Int  = ffi.javascript("k$x  + k$y").as(Int)

IntList = tags.
    case(.Nil) 
    case(.Cons($.current(Int) next(IntList)))

myList=IntList.Cons($.
    current (1) 
    next    (IntList.Cons($.
        current (2) 
        next    (IntList.Cons($.
            current (3) 
            next    (IntList.Nil))))))


(this IntList).length | Int =
    this.
        case(.Nil):
            (0) 

        case(.Cons(c)):
            (1.plus(c.next.length))

(this IntList).sum | Int = 
    this.
        case(.Nil):
            (0)

        case(.Cons(c)):
            (c.current.plus(c.next.sum))

=myList.length
=myList.sum