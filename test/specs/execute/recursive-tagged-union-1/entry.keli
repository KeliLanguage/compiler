(x Int).plus(y Int) | Int  = ffi.javascript("k$x  + k$y").as(Int)

IntList = choice
    .Nil
    .Cons($.current(Int) next(IntList))

myList=IntList.Cons($.
    current (1) 
    next    (IntList.Cons($.
        current (2) 
        next    (IntList.Cons($.
            current (3) 
            next    (IntList.Nil))))))


(this IntList).length | Int =
    this.
        if(.Nil) then
            (0) 

        if(.Cons(c)) then
            (1.plus(c.next.length))

(this IntList).sum | Int = 
    this.
        if(.Nil) then
            (0)

        if(.Cons(c)) then
            (c.current.plus(c.next.sum))

=myList.length
=myList.sum