    #(true) 
    #(false)

(x Int).minus   (y Int) | Int  = ffi.javascript("$x - $y").as(Int)
(x Int).times   (y Int) | Int  = ffi.javascript("$x * $y").as(Int)
(x Int).equals  (y Int) | Boolean = ffi.javascript("$x === $y ? $Boolean.$true : $Boolean.$false").as(Boolean)

(x Int).factorial | Int =  
    x.equals(0).
        if(true):
            (1)
        if(false):
            (x.times((x.minus(1)).factorial))

